mod test {
    pipe add_multiple(start: i32, add1: i32, add2: i32) -> (count: i32) 
    [
        (start: i32, add1: i32, add2: i32) {
            let value : i32 = start + add1;
            continue(args: (value: value, add2: add2), deps: []);
        }
        (value: i32, add2: i32) {
            let value : i32 = value + add2;
            continue(args: (count: value));
        }
    ]
}
