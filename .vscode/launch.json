{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "codelldb",
            "request": "launch",
            "name": "Cli-Debug",
            "preLaunchTask": "cargo-build-debug",
            "program": "${workspaceFolder}/target/debug/branec_cli",
            "sourceLanguages": [
                "rust"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/cli",
            "args": [
                "${workspaceFolder}/exampleScripts/test.bscript"
            ]
        },
        {
            "type": "codelldb",
            "request": "launch",
            "name": "Function-Calls-Debug",
            "preLaunchTask": "cargo-build-debug",
            "program": "${workspaceFolder}/target/debug/branec_cli",
            "sourceLanguages": [
                "rust"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/cli",
            "args": [
                "${workspaceFolder}/tests/object_function_calls.bscript"
            ]
        },
        {
            "type": "codelldb",
            "request": "launch",
            "name": "Cli-Release",
            "expressions": "native",
            "program": "cargo",
            "console": "internalConsole",
            "sourceLanguages": [
                "rust"
            ],
            "cwd": "${workspaceFolder}/cli",
            "args": [
                "run",
                "--release",
                "--",
                "${workspaceFolder}/exampleScripts/test.bscript"
            ]
        }
    ]
}
